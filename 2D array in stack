#include <iostream>
using namespace std;

int main() {
    int Array[3][4];

    for (int i = 0; i < 3; i++) {
        for (int j = 0; j < 4; j++) {
            if (i == 0) {
                Array[i][j] = j;
            } else {
                Array[i][j] = i * j;
            }
            cout << Array[i][j] << "  ";
        }
        cout << endl; // Add a newline after each row
    }

    return 0;
}
